'''

import pyautogui

temperatyra = int(input("Введите температуру: "))
vlaga = int(input("введите влажность: "))
pyautogui.hotkey('win', 'd')
pyautogui.doubleClick(1345,559) 
if temperatyra <= 10  and vlaga >= 60:
    pyautogui.hotkey('win', 'd')
    pyautogui.moveTo(516, 1062)
    pyautogui.doubleClick()
if temperatyra >= 30  and vlaga <= 30:
    pyautogui.hotkey('winleft', 'd')
    pyautogui.moveTo(516, 1062)
    pyautogui.doubleClick()

