if parol == "1234" and login == "yandex.ru":
    pyautogui.alert("секретная информация")
if parol != "1234" and login != "yandex.ru":
    pyautogui.alert("пароль и логин неверен")
if parol != "1234" and login == "yandex.ru":    
    pyautogui.alert("пароль неверен")
