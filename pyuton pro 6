'''
import pyautogui
imi = input("Введите имя: ")
famili = input("введите фамилию: ")
rost = int(input("введите возраст: "))
if imi != "Микордий" and famili != "Пармезанов" and rost >= 17:  
    pyautogui.alert("вы вступили в клуб")
if imi == "Микордий" and famili == "Пармезанов" and rost >= 17:  
    pyautogui.alert("вы в черном списке")
if imi != "Микордий" and famili != "Пармезанов" and rost <= 17:  
    pyautogui.alert("вы слишком маленький")
if imi == "Микордий" and famili =! "Пармезанов" and rost >= 17:  
    pyautogui.alert("вы в черном списке")
'''
'''
import pyautogui
